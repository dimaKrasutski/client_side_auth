{
  "extends": ["./node_modules/@scorum/theme-bootstrap/.eslintrc"],
  "settings": {
    "import/resolver": {
      "babel-module": {
        "theme": "@scorum/theme-bootstrap/src"
      }
    }
  },
  "rules": {
    "no-unused-expressions": ["error", { "allowShortCircuit": true }],
    "function-paren-newline": [0]
  }
}
